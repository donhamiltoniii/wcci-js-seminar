ets/styles/styles.css":"/* Font imports */\n@import url(\"https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,400;0,700;1,400;1,700&display=swap\");\n@import url(\"https://fonts.googleapis.com/css2?family=Pacifico&display=swap\");\n@import url(\"https://fonts.googleapis.com/css2?family=Roboto+Slab:wght@600&display=swap\");\n\n/* Style Reset Rules */\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\n\nhtml {\n  --xiketic: #100007;\n  --dark-purple: #200116;\n  --dark-sienna: #2d0605;\n  --grey: #ddd;\n  --medium-aquamarine: #80d39b;\n  --tyrian-purple: #4c0827;\n  font: 20px/1 Montserrat, sans-serif;\n}\n\na {\n  color: var(--dark-purple);\n  text-decoration: none;\n}\n\narticle {\n  margin-bottom: 1rem;\n}\n\nh1,\nh2,\nh3,\nh4,\nh5,\nh6 {\n  border-bottom: 1px solid var(--grey);\n  color: var(--tyrian-purple);\n  font-family: \"Roboto Slab\", sans-serif;\n  font-weight: 700;\n  line-height: 1.618em;\n  margin-bottom: 0.75rem;\n  padding-bottom: 0.325rem;\n}\n\nimg {\n  width: 100%;\n}\n\nul {\n  list-style: none;\n  padding: 0;\n}\n\n/* Utility Classes */\n.btn {\n  background-color: var(--xiketic);\n  border: none;\n  color: white;\n  font-size: 0.5rem;\n  padding: 0.5rem;\n}\n\n.container {\n  margin: 0 auto;\n  max-width: 30rem;\n  width: 90%;\n}\n\n.hidden {\n  display: none;\n}\n\n/* App Styles */\n.about {\n  display: flex;\n}\n\n.about__avatar-container {\n  align-items: center;\n  border: 3px solid var(--xiketic);\n  border-radius: 50%;\n  height: 5rem;\n  margin-right: 1rem;\n  width: 5rem;\n  overflow: hidden;\n}\n\n.about__user-info {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n}\n\n.about__user-info > * {\n  border: none;\n  margin-bottom: 0;\n  padding-bottom: 0;\n}\n\n.comments__list {\n  padding: 1rem;\n}\n\n.comments__list-item {\n  border-bottom: 1px solid var(--medium-aquamarine);\n  margin-bottom: 1rem;\n  padding-bottom: 1rem;\n}\n\n.comments__list-item:last-child {\n  border-bottom: none;\n}\n\n.details__title {\n  display: flex;\n  justify-content: space-between;\n}\n\n.details__list {\n  padding: 1rem;\n}\n\n.details__list li {\n  border-bottom: 1px solid var(--medium-aquamarine);\n  padding-bottom: 1rem;\n  margin-bottom: 1rem;\n}\n\n.details__list li:last-child {\n  border: none;\n}\n\n.main-footer {\n  align-items: center;\n  background-color: var(--xiketic);\n  color: white;\n  display: flex;\n  justify-content: center;\n  padding: 3rem 1rem 5rem;\n}\n\n.main-header {\n  align-items: center;\n  border-bottom: 1px solid var(--medium-aquamarine);\n  display: flex;\n  justify-content: space-between;\n  padding: 1rem;\n  margin-bottom: 3rem;\n}\n\n.main-header__logo h1 {\n  border: none;\n  color: var(--tyrian-purple);\n  font-size: 1.5rem;\n  margin-bottom: 0;\n  padding-bottom: 0;\n}\n\n.main-header__logo h2 {\n  border: none;\n  font-size: 0.875rem;\n  margin-bottom: 0;\n  padding-bottom: 0;\n}\n\n.main-header__nav {\n  position: relative;\n}\n\n.main-header__nav-list {\n  position: absolute;\n  left: 0;\n  top: 1.5rem;\n}\n\n.posts__add-post {\n  display: flex;\n  flex-direction: column;\n  margin: 1rem 0;\n}\n\n.posts__add-post > *:not(h4) {\n  margin-bottom: 1rem;\n  padding: 0.5rem;\n}\n\n.posts__add-post > h4 {\n  margin-bottom: 1em;\n}\n\n.post__list-item {\n  background-color: var(--grey);\n  padding: 1rem;\n}\n\n.post__list-item:nth-child(even) {\n  background-color: white;\n  padding: 1rem;\n}\n"},"lineCount":202}},"error":null,"hash":"d98f9dbfb7873178422204edc54d1148","cacheData":{"env":{}}}